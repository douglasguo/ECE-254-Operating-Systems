N      printf ("   Received: %d\n", received);
N    }
N    
N    consumed++;
N    
N    //Housekeeping
N    os_mut_release (mutex_mailbox);
X    _os_mut_release((U32)rt_mut_release,mutex_mailbox);
N    os_sem_send (semWrite);
X    _os_sem_send((U32)rt_sem_send,semWrite);
N    
N    if (consumed == N) break;
X    if (consumed == 100) break;
N    
N    os_dly_wait (DELAY);
X    _os_dly_wait((U32)rt_dly_wait,10);
