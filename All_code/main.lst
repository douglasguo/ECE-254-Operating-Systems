N__task void Producer(void)
X__declspec(noreturn) void Producer(void)
N{
N  char Message[5];
N  int pid = PID++;
N  
N  os_dly_wait (5);
X  _os_dly_wait((U32)rt_dly_wait,5);
N  mq_open("Lab2Queue", 0);
X  _mq_open((U32)rt_mq_open,"Lab2Queue",0);
N  
N  for (EVER)
X  for (;;)
N  {
N    if ((produced % P) == pid)
X    if ((produced % 1) == pid)
N    {
N      //Wait for the mailbox to have free space
N      os_sem_wait (semWrite, 0xffff);
X      _os_sem_wait((U32)rt_sem_wait,semWrite,0xffff);
N      
N      //Wait for the ciritcal section to be available
N      os_mut_wait (mutex_mailbox, 0xffff);
X      _os_mut_wait((U32)rt_mut_wait,mutex_mailbox,0xffff);
N      
N      sprintf (Message, "%d", produced);
N      Message[4] =0;
N      mq_send(QID,Message,5,1);
N      
N//	    printf("   Message sent: %d\n",produced);
N      
N      produced++;
N      
N      //Housekeeping
N      os_mut_release (mutex_mailbox);
X      _os_mut_release((U32)rt_mut_release,mutex_mailbox);
N      os_sem_send (semRead);
X      _os_sem_send((U32)rt_sem_send,semRead);
N    }
N    
N    if (produced == N) break;
X    if (produced == 100) break;
N    
N    os_dly_wait (DELAY);
X    _os_dly_wait((U32)rt_dly_wait,10);
N  }
N  
N  os_tsk_delete_self ();
X  _os_tsk_delete((U32)rt_tsk_delete, 0);
N}
