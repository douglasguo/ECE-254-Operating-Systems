N__task void Consumer(void)
X__declspec(noreturn) void Consumer(void)
N{
N  char Message[5];
N  int cid = CID++;
N  int root, received;
N  int compare;
N  size_t msglen;
N  unsigned int priority;
N  
N  mq_open("Lab2Queue", 0);
X  _mq_open((U32)rt_mq_open,"Lab2Queue",0);
N  
