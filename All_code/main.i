__declspec(noreturn) void Producer(void)
{
  char Message[5];
  int pid = PID++;
  
  _os_dly_wait((U32)rt_dly_wait,5);
  _mq_open((U32)rt_mq_open,"Lab2Queue",0);
  
  for (;;)
  {
    if ((produced % 1) == pid)
    {
      
      _os_sem_wait((U32)rt_sem_wait,semWrite,0xffff);
      
      
      _os_mut_wait((U32)rt_mut_wait,mutex_mailbox,0xffff);
      
      sprintf (Message, "%d", produced);
      Message[4] =0;
      mq_send(QID,Message,5,1);
      

      
      produced++;
      
      
      _os_mut_release((U32)rt_mut_release,mutex_mailbox);
      _os_sem_send((U32)rt_sem_send,semRead);
    }
    
    if (produced == 100) break;
    
    _os_dly_wait((U32)rt_dly_wait,10);
  }
  
  _os_tsk_delete((U32)rt_tsk_delete, 0);
}

__declspec(noreturn) void Consumer(void)
{
  char Message[5];
  int cid = CID++;
  int root, received;
  int compare;
  size_t msglen;
  unsigned int priority;
  
  _mq_open((U32)rt_mq_open,"Lab2Queue",0);
  
  for (;;)
  {
    
    _os_sem_wait((U32)rt_sem_wait,semRead,0xffff);
    
    
    _os_mut_wait((U32)rt_mut_wait,mutex_mailbox,0xffff);
    
  	mq_receive (QID,Message,msglen,&priority);
    
    
    received = strtol (Message,0,10);
    root = sqrt (received);
    compare = root*root;
    
    if (compare == received)
    {
      printf ("   Received: %d\n", received);
    }
    
    consumed++;
    
    
    _os_mut_release((U32)rt_mut_release,mutex_mailbox);
    _os_sem_send((U32)rt_sem_send,semWrite);
    
    if (consumed == 100) break;
    
    _os_dly_wait((U32)rt_dly_wait,10);
  }
  
  finalize ();
  
  _os_tsk_delete((U32)rt_tsk_delete, 0);
}



 
__declspec(noreturn) void clock (void)
{
  _os_itv_set((U32)rt_itv_set,10);
  
  for (;;)
  {
    decimal++;
    
    if (decimal == 10)
    {
      second++;
      decimal = 0;
    }
    
    _os_itv_wait((U32)rt_itv_wait);
  }
}

void finalize (void)
{
  
  
  _os_mut_wait((U32)rt_mut_wait,mutex_timing,0xffff);
  
  
  if (!timingDisplayed)
  {
    
    endSec = second;
    endDec = decimal;
    
    
    
    
    printf ("Init: %u.%04u s", startSec, startDec);
    
    
    
    
    printf ("Done: %u.%04u s", endSec, endDec);
    
    timingDisplayed = 1;
  }
  
  _os_mut_release((U32)rt_mut_release,mutex_timing);
}



 
