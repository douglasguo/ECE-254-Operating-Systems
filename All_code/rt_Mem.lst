N  			//create children to split block
N  			
N  			if (tree->inuse != 0)
N  			{
N  				return 0;
N  			}
N  			
N  			//allocate memory for children
N  			toaddleft = root + (tree->index << 1);
N  			toaddright = root + (tree->index << 1) + 1;
N  			
N  			//create left child
N  			toaddleft->left = 0;
N  			toaddleft->right = 0;
N  			toaddleft->inuse = 0;
N  			toaddleft->startloc = tree->startloc;
N  			toaddleft->endloc = (void*) ((((unsigned int) tree->endloc - (unsigned int) tree->startloc + 1) / 2) - 1 + (unsigned int) tree->startloc);
N        toaddleft->index = (tree->index << 1);
N  			
N  			tree->left = toaddleft;
N  			
N  			//create right child
N  			toaddright->left = 0;
N  			toaddright->right = 0;
N  			toaddright->inuse = 0;
N  			toaddright->startloc = (void*) ((((unsigned int) tree->endloc - (unsigned int) tree->startloc + 1) / 2) + (unsigned int) tree->startloc);
N  			toaddright->endloc = tree->endloc;
N  			toaddright->index = (tree->index << 1) + 1;
N        
N  			tree->right = toaddright;
N  			
N  			//created children now attempt to allocate in one
N  			return allocate_block (tree->left, size,treelevel + 1);
N      }
N      else
N      {
N        //right block size
N  			//check if this memory is in use
N  			if (tree->inuse)
N  			{
N  				return 0; //in use can't use this
N  			}
N  			else
N  			{
N  				//allocate memory
N  				tree->inuse = 1;
N          
N  				return tree->startloc;
N  			}
N      }
N    }
N		else
N		{
N			return 0; //no space in memory
N		}
N	}
N}
N
N/*--------------------------- rt_free_box -----------------------------------*/
N
N/**
N * @brief: free memory pointed by ptr
N * @return: 0 on success and -1 otherwise.
N **/
Nint rt_mem_free (void* ptr)
N{
N  int result;
N  int taskOn;
N  P_TCB taskContext;
N  
N  if (!initmem ) //if silly programmer tries to free mem before allocating it 
N  {
N    return -1;
N  }
N  
N  result = freeblock(root,ptr);
N  
N  cleantree(root);
N  
N  if(result) //if some memory has been freed try to allocate this memory to any tasks waiting for memory
N  {
N    for (taskOn = 0; taskOn < numBlocked; taskOn++)
N	  {
N		  //some memory has been freed dispatch all tasks to try and allocate memory	
N			//set task into ready state and change its return value
N
N			taskContext = os_active_TCB[blockedTasks[taskOn] - 1];
N			taskContext->state = READY;
X			taskContext->state = 1;
N      rt_put_prio(&os_rdy,taskContext);
N	  }
N      
N	  numBlocked = 0;
N  }  
N  
N  //Note: To handle recursion the actual free algorithm returns 0 and 1 respectively.
N  return (--result);
N}
N
N/**
N *@brief: Release the hounds! (err... memory)
N *@return: 1 if the memory was found and removed, 0 otherwise
N **/
Nint freeblock (tree_node *tree, void* startofmem)
N{
N  if (tree->left != 0)
N	{
N		return (freeblock (tree->left, startofmem) || freeblock (tree->right, startofmem));
N	}
N	else //no children must be in leaf node
N	{
N		if (tree->startloc == startofmem)
N		{
N			tree->inuse = 0;
N			return 1;
N		}
N		else
N		{
N			return 0;
N		}
N	}
N}
N
N/**
N * @brief: Remove all dead branches from the tree
N * @return: The number of times the cleanleaves removed dead leaves.
N **/
Nint cleantree(tree_node *tree)
N{
N	int cleancounter = 0;
N	
N	while(cleanleaves(tree))
N	{
N		++cleancounter;
N	}
N	
N	return cleancounter;
N}
N
N/**
N * @brief: Called multiple times to recursively scan the tree for dead leaves and eliminate them
N * @return: 1 if any nodes were removed, 0 otherwise.
N **/
Nint cleanleaves(tree_node *tree)
N{
N	int flagLeft, flagRight;
N	int result = 0;
N	
N	//if parent has both children with taskid of 0 delete them
N	
N	//first check if children exist
N	if (tree->left != 0)
N	{
N    //is left tree in use
N    flagLeft = tree->left->inuse;
N    
N    //left tree is not in use, does it have children
N    if (flagLeft == 0)
N    {
N      flagLeft = (tree->left->left != 0);
N    }
N    
N    //is right tree in use
N    flagRight = tree->right->inuse;
N    
N    //right tree is not in use, does it have children
N    if (flagRight == 0)
N    {
N      flagRight = (tree->right->left != 0);
N    }
N    
N    //Dead tree, delete it
N    if (flagLeft == 0 && flagRight == 0)
N    {
N      tree->left = 0;
N      tree->right = 0;
N      
N      return 1;
N    }
N    else
N    {
N    	//Clean the trees that have children
N			if (tree->left->left != 0)
N	    {
N    		result = cleanleaves (tree->left);
N    	}
N    	
N    	if (tree->right->left != 0)
N    	{
N    		result = result | cleanleaves (tree->right);
N			}
N		  
N		  return result;
N    }
N	}
N	else
N	{
N		return 0; 
N  }
N}
N
