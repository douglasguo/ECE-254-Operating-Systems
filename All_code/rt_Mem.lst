N/**
N * @brief: Output the contents of the tree to the "console".
N **/
Nvoid print_tree (tree_node* tree)
N{
N 	if(tree->left)
N  {
N    print_tree (tree->left);
N  }
N  
N  printf ("StartLocation: %d | EndLocation: %d | In Use: %d | Array Position: %d \n", ((unsigned int) tree->startloc - (unsigned int)root->startloc) , ((unsigned int) tree->endloc - (unsigned int)root->startloc), tree->inuse, tree->index);
N  
N  if(tree->right)
N  {
N    print_tree (tree->right);
N  }
N}
